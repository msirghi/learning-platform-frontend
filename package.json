{
  "name": "learning-platform-frontend",
  "version": "0.1.9",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test:local": "npm run preTest && jest",
    "preTest": "bash ./common/scripts/preTest.sh true",
    "preStart": "node ./common/preStartHook.ts",
    "test:cov": "npm run preTest && jest --coverage",
    "test:update": "npm run test:cov -- -u",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "lint:test": "npm run lint && npm run test-cover",
    "test": "cross-env CI=true react-scripts test --silent --env=jsdom --coverage --testResultsProcessor jest-sonar-reporter",
    "sonar": "node sonar-project.js",
    "doc:pre": "bash ./common/scripts/preDocGen.sh react",
    "doc:gen": "npm run doc:pre && styleguidist server",
    "doc:build": "styleguidist build",
    "patch": "npm run version --patch"
  },
  "dependencies": {
    "@date-io/date-fns": "1.x",
    "@fullcalendar/core": "^5.5.1",
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/react": "^5.5.0",
    "@fullcalendar/timegrid": "^5.5.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.3.10",
    "@zeit/next-css": "^1.0.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "check-password-strength": "^1.0.15",
    "date-fns": "^2.19.0",
    "dateformat": "^4.4.1",
    "firebase": "^8.3.1",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "^4.17.21",
    "next": "10.0.0",
    "next-i18next": "^7.0.1",
    "next-pwa": "^5.0.3",
    "next-redux-wrapper": "^6.0.2",
    "next-transpile-modules": "^4.1.0",
    "nextjs-progressbar": "^0.0.7",
    "node-sass": "^5.0.0",
    "notistack": "^1.0.5",
    "path": "^0.12.7",
    "react": "17.0.1",
    "react-card-flip": "^1.1.0",
    "react-dom": "17.0.1",
    "react-google-recaptcha-v3": "^1.8.0",
    "react-hook-form": "^6.11.0",
    "react-redux": "^7.2.2",
    "react-spinners-kit": "^1.9.1",
    "redux": "^4.0.5",
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.1.1",
    "@testing-library/react-hooks": "^4.0.0",
    "@testing-library/user-event": "^12.7.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.1",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.21.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "mockdate": "^3.0.2",
    "prettier": "^2.2.1",
    "react-docgen-typescript": "1.16.2",
    "react-styleguidist": "^11.1.5",
    "react-test-renderer": "^17.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.29.0",
    "sass-loader": "10.1.1",
    "sonarqube-scanner": "^2.8.0",
    "source-map-loader": "0.2.4",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.17",
    "typescript": "^4.0.5"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "http://test.com/login"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "browser": {
    "fs": false,
    "child_process": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint & npm run test:local && npm --no-git-tag-version version patch && git add ."
    }
  }
}
